PROGRAM programTest2 IS
  INSTRUCTION instOne IS
    IF random THEN
      turnleft
    ELSE
      turnright
    END IF
  END instOne
  
  INSTRUCTION instTwo IS
    turnleft
    turnleft
  END instTwo

BEGIN
  WHILE true DO
    IF next-is-empty THEN
      move
    ELSE
      IF next-is-enemy THEN
        instTwo
    ELSE
      instOne
    END IF
  END IF
END WHILE
END programTest2
